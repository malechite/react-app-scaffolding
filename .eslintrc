{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "amd": true,
    "es6": true
  },
  "plugins": [
    "react",
  ],
  "rules": {
    "strict": 0,
    "brace-style": [2, "1tbs"],
    "camelcase": 0,
    "comma-dangle": [2, "never"],
    "semi": 2,
    "quotes": [2, "single", "avoid-escape"],
    "comma-spacing": [2, {"before": false, "after": true}],
    "comma-style": [2, "last"],
    "no-underscore-dangle": 0,
    "consistent-this": [2, "self"],
    "eqeqeq": [2, "smart"],
    "indent": 2,
    "key-spacing": 0,
    "new-cap": [2, {"capIsNew": false, "newIsCap": true}],
    "new-parens": 0,
    "no-cond-assign": 0,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-delete-var": 2,
    "no-debugger": 2,
    "no-dupe-keys": 2,
    "curly": 2,
    "eol-last": 0,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-new": 0,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 0,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-undef": 2,
    "no-undef-init": 2,
    "no-unreachable": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-warning-comments": [1, {"terms": ["todo", "fixme"]}],
    "quote-props": [0, "as-needed"],
    "radix": 2,
    "semi-spacing": [2, {"before": false, "after": true}],
    "keyword-spacing": 2,
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, "never"],
    "space-infix-ops": 2,
    "space-unary-ops": [2, {"words": true, "nonwords": false}],
    "use-isnan": 2,
    "valid-typeof": 2,
    "wrap-iife": [2, "inside"],
    "yoda": 0,

    //REACT RULES
    "react/display-name": 2,
    "react/jsx-boolean-value": 2,
    "jsx-quotes": [2, "prefer-single"],
    "react/jsx-indent": 2,
    "react/jsx-no-undef": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/no-did-mount-set-state": 2,
    "react/no-did-update-set-state": 2,
    "react/no-unknown-property": 2,
    "react/prop-types": 2,
    "react/react-in-jsx-scope": 2,
    "react/self-closing-comp": 2,
    "react/jsx-wrap-multilines": 2,
  },
  "globals": {
    "App": true,
    "ga": true,

    "mocha": true,
    "describe": true,
    "beforeAll": true,
    "beforeEach": true,
    "afterEach": true,
    "it": true,
    "expect": true,
    "fixture": true,
    "assert": true,
    "sinon": true
  }
}
